import java.io.*;

public class TestWrite {

  public static void main(String[] args) {
  
      try{
          FileWriter myfile = new FileWrite("D:\\test.csv");
          
           myfile.write("1,2,3,\n");
           myfile.write("4,5,6,\n");
           myfile.write("7,8,9,\n");
           
           myfile.close();
           }catch(IOException e) {
           
            System.out.println(e);
           }          
         }
         
}
